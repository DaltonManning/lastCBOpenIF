<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="CBOpenIFSchema3_0" targetNamespace="CBOpenIFSchema3_0" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:simpleType name="AuthenticationType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="None"/>
      <xs:enumeration value="Reauthenticate"/>
      <xs:enumeration value="Double authenticate"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:attributeGroup name="ObjInstSafetyAttributes">
    <xs:attribute name="AccessLevel" type="xs:string" default=""/>
    <xs:attribute name="SafetyType" type="xs:string" default=""/>
  </xs:attributeGroup>
  <xs:simpleType name="SILType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="NonSIL"/>
      <xs:enumeration value="SIL2"/>
      <xs:enumeration value="SIL3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DirectionType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="in"/>
      <xs:enumeration value="in_out"/>
      <xs:enumeration value="out"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CommVarDirectionType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="in"/>
      <xs:enumeration value="out"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CMParDirectionType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="in"/>
      <xs:enumeration value="in_out"/>
      <xs:enumeration value="out"/>
      <xs:enumeration value="unspecified"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="SignalDirectionType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="in"/>
      <xs:enumeration value="out"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ScopeType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="private"/>
      <xs:enumeration value="public"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="FDPortType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="yes"/>
      <xs:enumeration value="no"/>
      <xs:enumeration value="yes left"/>
      <xs:enumeration value="no left"/>
      <xs:enumeration value="yes right"/>
      <xs:enumeration value="no right"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CommVarIntervalTimeType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="very fast"/>
      <xs:enumeration value="fast"/>
      <xs:enumeration value="normal"/>
      <xs:enumeration value="slow"/>
      <xs:enumeration value="very slow"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CommVarPriority">
    <xs:restriction base="xs:string">
      <xs:enumeration value="high"/>
      <xs:enumeration value="normal"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CommVarExpectedSIL">
    <xs:restriction base="xs:string">
      <xs:enumeration value="NonSIL"/>
      <xs:enumeration value="SIL2"/>
      <xs:enumeration value="SIL3"/>
      <xs:enumeration value="same"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TreeRedundancyDisplayMode">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Position"/>
      <xs:enumeration value="Upper_Lower"/>
      <xs:enumeration value="A_B"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="Application">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ApplicationVariables" minOccurs="0"/>
        <xs:element ref="DataTypes" minOccurs="0"/>
        <xs:element ref="FunctionBlockTypes" minOccurs="0"/>
        <xs:element ref="ControlModuleTypes" minOccurs="0"/>
        <xs:element ref="DiagramTypes" minOccurs="0"/>
        <xs:element ref="ControlModules" minOccurs="0"/>
        <xs:element ref="Diagrams" minOccurs="0"/>
        <xs:element ref="Programs" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="TaskConnection" type="xs:string" default=""/>
      <xs:attribute name="Guid" type="xs:string" default=""/>
      <xs:attribute name="FilePath" type="xs:string" default=""/>
      <xs:attribute name="MajorVersion" type="xs:unsignedLong" use="required"/>
      <xs:attribute name="MinorVersion" type="xs:unsignedLong" use="required"/>
      <xs:attribute name="Revision" type="xs:unsignedLong" use="required"/>
      <xs:attribute name="SILLevel" type="SILType" default="NonSIL"/>
      <xs:attribute name="SimulationMark" type="xs:boolean" default="0"/>
      <xs:attribute name="ReservationState" type="xs:string" default=""/>
      <xs:attribute name="Folder" type="xs:string" use="optional" default=""/>
      <xs:attribute name="FolderPath" type="xs:string" use="optional" default=""/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ApplicationProperties">
    <xs:complexType>
      <xs:attribute name="Type" type="xs:string" default="Normal"/>
      <xs:attribute name="SILLevel" type="SILType" default="NonSIL"/>
      <xs:attribute name="SimulationMark" type="xs:boolean" default="0"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ApplicationVariables">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="GlobalVariables" minOccurs="0"/>
        <xs:element ref="Variables" minOccurs="0"/>
        <xs:element ref="Signals" minOccurs="0"/>
        <xs:element ref="Description" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Applications">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Folder" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="Application" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Folder">
    <xs:complexType>
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="Guid" type="xs:string"  use="optional" default=""/>
      <xs:attribute name="Parent" type="xs:string"  use="optional" default=""/>
      <xs:attribute name="ParentPath" type="xs:string" use="optional" default=""/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ExecutionInstance">
    <xs:complexType>
      <xs:attribute name="Name" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ExecutionGroup">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ExecutionInstance" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="TaskName" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ExecutionOrder">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ExecutionGroup" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CMConnection">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Point" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="ActualParameter" type="xs:string" default=""/>
      <xs:attribute name="GraphicalConnection" type="xs:boolean" default="0"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="CMConnections">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="CMConnection" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="FDConnection">
    <xs:complexType>
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="ActualParameter" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="CodeBlocks">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="STCodeBlock"/>
        <xs:element ref="FBDCodeBlock"/>
        <xs:element ref="LDCodeBlock"/>
        <xs:element ref="SFCCodeBlock"/>
        <xs:element ref="ILCodeBlock"/>
        <xs:element ref="FDCodeBlock"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="Component">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Description" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="Type" type="xs:string" use="required"/>
      <xs:attribute name="Attribute" type="xs:string" default="retain"/>
      <xs:attribute name="InitialValue" type="xs:string" default=""/>
      <xs:attribute name="ISPValue" type="xs:string" default=""/>
      <xs:attribute name="ReadPermission" type="xs:string" default=""/>
      <xs:attribute name="WritePermission" type="xs:string" default=""/>
      <xs:attribute name="AuthenticationLevel" type="AuthenticationType" default="None"/>
      <xs:attribute name="TypeGuid" type="xs:string" default=""/>
      <xs:attribute name="TypePath" type="xs:string" default=""/>
      <xs:attributeGroup ref="ObjInstSafetyAttributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Components">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Component" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ConVariable">
    <xs:complexType mixed="true">
      <xs:attribute name="Signal" type="xs:boolean" default="0"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ConnectedHardwareLibrary">
    <xs:complexType>
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="MajorVersion" type="xs:unsignedLong" use="required"/>
      <xs:attribute name="MinorVersion" type="xs:unsignedLong" use="required"/>
      <xs:attribute name="Revision" type="xs:unsignedLong" use="required"/>
      <xs:attribute name="Guid" type="xs:string" use="optional" default=""/>
      <xs:attribute name="FilePath" type="xs:string" use="optional" default=""/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ConnectedHardwareLibraries">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ConnectedHardwareLibrary" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ConnectedApplication">
    <xs:complexType mixed="true">
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="MajorVersion" type="xs:unsignedLong" default="1"/>
      <xs:attribute name="MinorVersion" type="xs:unsignedLong" default="0"/>
      <xs:attribute name="Revision" type="xs:unsignedLong" default="0"/>
      <xs:attribute name="Guid" type="xs:string" default=""/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ConnectedApplications">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ConnectedApplication" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ControlModule">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="CMInstGraphics" minOccurs="0"/>
        <xs:element ref="CMConnections" minOccurs="0"/>
        <xs:element ref="FunctionBlocks" minOccurs="0"/>
        <xs:element ref="ControlModules" minOccurs="0"/>
        <xs:element ref="Description" minOccurs="0"/>
        <xs:element ref="GraphPos" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="Type" type="xs:string" use="required"/>
      <xs:attribute name="TaskConnection" type="xs:string" default=""/>
      <xs:attribute name="VisibilityInGraphics" default="default">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="default"/>
            <xs:enumeration value="visible"/>
            <xs:enumeration value="invisible"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="Guid" type="xs:string" default=""/>
      <xs:attribute name="TypeGuid" type="xs:string" default=""/>
      <xs:attribute name="TypePath" type="xs:string" default=""/>
      <xs:attribute name="AspectObject" type="xs:boolean" default="1"/>
      <xs:attribute name="ExposePropertiesInParent" type="xs:boolean" default="false"/>
      <xs:attributeGroup ref="ObjInstSafetyAttributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ControlModuleType">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="CMTypeGraphics" minOccurs="0"/>
        <xs:element ref="CMParameters" minOccurs="0"/>
        <xs:element ref="Variables" minOccurs="0"/>
        <xs:element ref="ExternalVariables" minOccurs="0"/>
        <xs:element ref="FunctionBlocks" minOccurs="0"/>
        <xs:element ref="ControlModules" minOccurs="0"/>
        <xs:element ref="CodeBlocks" minOccurs="0"/>
        <xs:element ref="Description" minOccurs="0"/>
        <xs:element ref="GraphSize" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="Protected" type="xs:boolean" default="0"/>
      <xs:attribute name="Hidden" type="xs:boolean" default="0"/>
      <xs:attribute name="Scope" type="ScopeType" default="public"/>
      <xs:attribute name="InteractionWindow" type="xs:string" default=""/>
      <xs:attribute name="AlarmOwner" type="xs:boolean" default="0"/>
      <xs:attribute name="InstantiateAsAspectObject" type="xs:boolean" default="0"/>
      <xs:attribute name="BatchObject" type="xs:string" default=""/>
      <xs:attribute name="Guid" type="xs:string" default=""/>
      <xs:attribute name="SILLevel" type="SILType" default="NonSIL"/>
      <xs:attribute name="RestrictedSIL" type="xs:boolean" default="0"/>
      <xs:attribute name="SimulationMark" type="xs:boolean" default="0"/>
      <xs:attribute name="ProductMask" type="xs:unsignedLong"/>
      <xs:attribute name="CreateEventAspect" type="xs:boolean"/>
      <xs:attribute name="SafeP2PObject" type="xs:string" default="None"/>
      <xs:attribute name="ReservedByFunction" type="xs:string" default=""/>
      <xs:attribute name="EmbeddedGraphicsVisible" type="xs:boolean" default="0"/>
      <xs:attribute name="ReservationState" type="xs:string" default=""/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ControlModuleTypes">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ControlModuleType" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ControlModules">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="ControlModule"/>
        <xs:element ref="SingleControlModule"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="Diagram">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Variables" minOccurs="0"/>
        <xs:element ref="CommVariables" minOccurs="0"/>
        <xs:element ref="Signals" minOccurs="0"/>
        <xs:element ref="FunctionBlocks" minOccurs="0"/>
        <xs:element ref="ControlModules" minOccurs="0"/>
        <xs:element ref="DiagramInstances" minOccurs="0"/>
        <xs:element ref="InitValues" minOccurs="0"/>
        <xs:element ref="CodeBlocks" minOccurs="0"/>
        <xs:element ref="Description" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="BatchObject" type="xs:string" default=""/>
      <xs:attribute name="TaskConnection" type="xs:string" default=""/>
      <xs:attribute name="TypeGuid" type="xs:string" default=""/>
      <xs:attribute name="InstGuid" type="xs:string" default=""/>
      <xs:attribute name="SILLevel" type="SILType" default="NonSIL"/>
      <xs:attribute name="RestrictedSIL" type="xs:boolean" default="0"/>
      <xs:attribute name="SimulationMark" type="xs:boolean" default="0"/>
      <xs:attribute name="ReservedByFunction" type="xs:string" default=""/>
      <xs:attributeGroup ref="ObjInstSafetyAttributes"/>
      <xs:attribute name="ReservationState" type="xs:string" default=""/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Diagrams">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref ="Diagram" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="DiagramType">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Parameters" minOccurs="0"/>
        <xs:element ref="Variables" minOccurs="0"/>
        <xs:element ref="FunctionBlocks" minOccurs="0"/>
        <xs:element ref="ControlModules" minOccurs="0"/>
        <xs:element ref="DiagramInstances" minOccurs="0"/>
        <xs:element ref="CodeBlocks" minOccurs="0"/>
        <xs:element ref="Description" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="Protected" type="xs:boolean" default="0"/>
      <xs:attribute name="Hidden" type="xs:boolean" default="0"/>
      <xs:attribute name="Scope" type="ScopeType" default="public"/>
      <xs:attribute name="AlarmOwner" type="xs:boolean" default="0"/>
      <xs:attribute name="InstantiateAsAspectObject" type="xs:boolean" default="0"/>
      <xs:attribute name="BatchObject" type="xs:string" default=""/>
      <xs:attribute name="Guid" type="xs:string" default=""/>
      <xs:attribute name="SILLevel" type="SILType" default="NonSIL"/>
      <xs:attribute name="RestrictedSIL" type="xs:boolean" default="0"/>
      <xs:attribute name="SimulationMark" type="xs:boolean" default="0"/>
      <xs:attribute name="ReservedByFunction" type="xs:string" default=""/>
      <xs:attribute name="EmbeddedGraphicsVisible" type="xs:boolean" default="0"/>
      <xs:attribute name="ReservationState" type="xs:string" default=""/>
    </xs:complexType>
  </xs:element>
  <xs:element name="DiagramTypes">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="DiagramType" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="DiagramInstances">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="DiagramInstance" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="DiagramInstance">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="FunctionBlocks" minOccurs="0"/>
        <xs:element ref="ControlModules" minOccurs="0"/>
        <xs:element ref="DiagramInstances" minOccurs="0"/>
        <xs:element ref="Description" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="Type" type="xs:string" use="required"/>
      <xs:attribute name="Guid" type="xs:string" default=""/>
      <xs:attribute name="TypeGuid" type="xs:string" default=""/>
      <xs:attribute name="TypePath" type="xs:string" default=""/>
      <xs:attribute name="AspectObject" type="xs:boolean" default="1"/>
      <xs:attribute name="ExposePropertiesInParent" type="xs:boolean" default="false"/>
      <xs:attributeGroup ref="ObjInstSafetyAttributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Controller">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="HWUnits" minOccurs="0"/>
        <xs:element ref="Tasks" minOccurs="0"/>
        <xs:element ref="ConnectedApplications" minOccurs="0"/>
        <xs:element ref="ConnectedHardwareLibraries" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="Guid" type="xs:string" default=""/>
      <xs:attribute name="FilePath" type="xs:string" default=""/>
      <xs:attribute name="SystemIdentity" type="xs:string" use="required"/>
      <xs:attribute name="ControllerType" type="xs:string" use="required"/>
      <xs:attribute name="MajorVersion" type="xs:unsignedLong" use="required"/>
      <xs:attribute name="MinorVersion" type="xs:unsignedLong" use="required"/>
      <xs:attribute name="Revision" type="xs:unsignedLong" use="required"/>
      <xs:attribute name="ReservationState" type="xs:string" default=""/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Controllers">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Controller" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ControllerProperties">
    <xs:complexType>
      <xs:attribute name="HWSimulation" type="xs:boolean" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="CommonControllerSettings">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="FatalOverrun"/>
        <xs:choice>
          <xs:element ref="LoadBalancing"/>
          <xs:element ref="Safety"/>
        </xs:choice>
        <xs:element ref="ErrorHandler"/>
      </xs:sequence>
      <xs:attribute name="ChangeTime" type="xs:string" default=""/>
      <xs:attribute name="OLUStopTime" type="xs:unsignedLong" use="optional" default="3000"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="FatalOverrun">
    <xs:complexType>
      <xs:attribute name="Limit" type="xs:unsignedLong" use="required"/>
      <xs:attribute name="Reaction" type="xs:unsignedLong" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="LoadBalancing">
    <xs:complexType>
      <xs:attribute name="OverLoadCompensation" type="xs:boolean" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Safety">
    <xs:complexType>
      <xs:attribute name="DemandMode" type="xs:unsignedLong" use="required"/>
      <xs:attribute name="FDRT" type="xs:unsignedLong" use="required"/>
      <xs:attribute name="SafetyWatchdogTimeout" type="xs:unsignedLong" use="optional" default="2000"/>
      <xs:attribute name="SIL3AppStartValUpdInterval" type="xs:unsignedLong" use="optional" default="86400"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ErrorHandler">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="SystemDiagnostics"/>
        <xs:element ref="Execution"/>
        <xs:element ref="IO"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:group name="ErrorHandlerGroup">
    <xs:sequence>
      <xs:element ref="Log"/>
      <xs:element ref="Event"/>
      <xs:element ref="Reset"/>
      <xs:element ref="SystemAlarmOutput" minOccurs="0"/>
    </xs:sequence>
  </xs:group>
  <xs:element name="SystemDiagnostics">
    <xs:complexType>
      <xs:sequence>
        <xs:group ref="ErrorHandlerGroup"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Execution">
    <xs:complexType>
      <xs:sequence>
        <xs:group ref="ErrorHandlerGroup"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="IO">
    <xs:complexType>
      <xs:sequence>
        <xs:group ref="ErrorHandlerGroup"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="ErrorHandlerAttributeGroup">
    <xs:attribute name="Low" type="xs:boolean"/>
    <xs:attribute name="Medium" type="xs:boolean"/>
    <xs:attribute name="High" type="xs:boolean"/>
    <xs:attribute name="Critical" type="xs:boolean"/>
    <xs:attribute name="Fatal" type="xs:boolean"/>
  </xs:attributeGroup>
  <xs:element name="Log">
    <xs:complexType>
      <xs:attributeGroup ref="ErrorHandlerAttributeGroup"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Event">
    <xs:complexType>
      <xs:attributeGroup ref="ErrorHandlerAttributeGroup"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Reset">
    <xs:complexType>
      <xs:attributeGroup ref="ErrorHandlerAttributeGroup"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="SystemAlarmOutput">
    <xs:complexType>
      <xs:attributeGroup ref="ErrorHandlerAttributeGroup"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="DataType">
